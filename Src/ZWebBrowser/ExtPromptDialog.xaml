                <!--UIElementCollection-->
                    <TextBox x:Name="extBox" Header="Open with ext" IsSpellCheckEnabled="True" />
                <!--/UIElementCollection-->
            <!--/Children-->
        <!--/StackPanel-->
    </Grid>
</ContentDialog>
